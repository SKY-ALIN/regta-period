[MESSAGES CONTROL]
disable=missing-docstring, too-few-public-methods, multiple-statements, invalid-name, invalid-overridden-method, too-many-arguments, too-many-public-methods, anomalous-backslash-in-string

[REPORTS]
score=no

[FORMAT]
max-line-length=120

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no